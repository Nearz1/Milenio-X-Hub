local Players = game:GetService("Players")
local kickk = Players.LocalPlayer
local motivo = "MILENIO X HUB: our script has been discontinued we are making a new version stay tuned at https://discord.gg/vdCm8pxS9A"

local function kickPlayer(player, motivo)
    if player and player:IsA("Player") then
        player:Kick(motivo)
    end
end

kickPlayer(kickk, motivo)
